{
    "name": "sachinkiranti/easelog",
    "description": "A simple yet flexible event log activity integration system for laravel.",
    "type": "php",
    "license": "MIT",
    "authors": [
        {
            "name": "sachinkiranti",
            "email": "sachinkiranti@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : "^7.0",
        "illuminate/support": "~5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*|^6.3",
        "orchestra/testbench": "~3.4.0|~3.5.0"
    },
    "autoload": {
        "psr-4": {
            "SachinKiranti\\Easelog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SachinKiranti\\Easelog\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SachinKiranti\\Easelog\\EaselogServiceProvider"
            ],
            "aliases": {
                "Easelog": "SachinKiranti\\Easelog\\EaselogFacade"
            }
        }
    }
}
